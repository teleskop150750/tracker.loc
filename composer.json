{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "beberlei/assert": "^3.3",
        "fruitcake/laravel-cors": "^3.0",
        "laravel-doctrine/orm": "^1.8",
        "laravel/lumen-framework": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.11",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "schema:update": "php artisan doctrine:schema:update",
        "schema:validate": "php artisan doctrine:schema:validate",
        "doctrine:info": "php artisan doctrine:info",
        "db:wipe": "php artisan db:wipe",
        "migrate": "php artisan migrate",
        "refresh": "php artisan migrate:refresh",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
